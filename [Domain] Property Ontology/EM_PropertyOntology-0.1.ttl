#-- Authentication Mode
  
prop:AuthenticationMode a owl:Class ;
        rdfs:label "Authentication Mode"@en ;      
        rdfs:comment """The authentication mode refers to the method or process used to verify and authenticate the identity and authorization of users accessing related infrastructure."""@en ;
        rdfs:subClassOf prop:QualitativeProperty;
        rdfs:isDefinedBy  prop: .

prop:LocalAuthenticationMode a owl:Class ;
        rdfs:label "Local Authentication Mode"@en ;      
        rdfs:comment """The local authentication mode refers to a method of verifying and authorizing users or devices within an infrastructure without requiring communication with an external server or network."""@en ;
        rdfs:subClassOf prop:AuthenticationMode;
        rdfs:isDefinedBy  prop: .

prop:RemoteAuthenticationMode a owl:Class ;
        rdfs:label "Remote Authentication Mode"@en ;      
        rdfs:comment """The remote authentication mode refers to a method of verifying and granting access to users or devices that requires communication with external authentication servers or services over a network."""@en ;
        rdfs:subClassOf prop:AuthenticationMode;
        rdfs:isDefinedBy  prop: .

prop:NoAuthenticationMode a owl:Class ;
        rdfs:label "No Authentication Mode"@en ;      
        rdfs:comment """The no authentication mode is a configuration in which a system or network or service operated without implementing any authentication mechanisms."""@en ;
        rdfs:subClassOf prop:AuthenticationMode;
        rdfs:isDefinedBy  prop: .

prop:RFIDAuthenticationMode a owl:Class ;
        rdfs:label "RFID Authentication Mode"@en ;      
        rdfs:comment """The Radio-Frequency Identification (RFID) refers to a method of authorization to a system, facility, or service using RFID technology."""@en ;
        rdfs:subClassOf prop:LocalAuthenticationMode;
        rdfs:isDefinedBy  prop: .

prop:AuthenticationBy15118 a owl:Class ;
        rdfs:label "Authentication By 15118"@en ;      
        rdfs:comment """The authentication by 15118 refers to a specific authentication mechanism defined in the ISO 15118 standard."""@en ;
        rdfs:subClassOf prop:LocalAuthenticationMode;
        rdfs:isDefinedBy  prop: .

#--  Charging Speed

prop:ChargingSpeed a owl:Class ;
        rdfs:label "Charging Speed Property"@en ;
        rdfs:comment """The class of charging speed refers to the rate at which a system's battery can be recharged with electric energy."""@en ;
        rdfs:subClassOf prop:QualitativeProperty;
        rdfs:isDefinedBy prop: .

prop:StandardChargingSpeed a owl:NamedIndividual, prop:ChargingSpeed;
        rdfs:label "Standard Charging Speed"@en  ;
        rdfs:comment "Standard charging speed. "@en ;
        rdfs:isDefinedBy prop: .

prop:AcceleratedChargingSpeed a owl:NamedIndividual , prop:ChargingSpeed;
        rdfs:label "Accelerated Charging Speed"@en ;
        rdfs:comment "Accelerated charging speed. "@en ;
        rdfs:isDefinedBy prop: .

prop:FastChargingSpeed a owl:NamedIndividual , prop:ChargingSpeed;
        rdfs:label "Fast Charging Speed"@en  ;
        rdfs:comment "Fast charging speed. "@en ;
        rdfs:isDefinedBy prop: .

prop:UltraFastChargingSpeed a owl:NamedIndividual , prop:ChargingSpeed;
        rdfs:label "Ultra Fast Charging Speed"@en  ;
        rdfs:comment "Ultra Fast charging speed. "@en ;
        rdfs:isDefinedBy prop: .

prop:ExtremelyFastChargingSpeed a owl:NamedIndividual , prop:ChargingSpeed;
        rdfs:label "Extremely Fast Charging Speed"@en  ;
        rdfs:comment "Extremely Fast Fast charging speed. "@en ;
        rdfs:isDefinedBy prop: .

#-- Payment Method

prop:PaymentMethod a owl:Class ;
        rdfs:label "Payment Method"@en ;      
        rdfs:comment """The payment method refers to the various ways can pay for services, infrastructure use, or offerings."""@en ;
        rdfs:subClassOf prop:QualitativeProperty;
        rdfs:isDefinedBy  prop: .
         
prop:FreePaymentMethod a owl:Class ;
        rdfs:label "Free Payment Method"@en ;      
        rdfs:comment """The free payment method reefrs to a transactional process where the payer does not incur any direct financial cost or obligation when making a payment."""@en ;
        rdfs:subClassOf prop:PaymentMethod;
        rdfs:isDefinedBy  prop: .

prop:CreditCardPaymentMethod a owl:Class ;
        rdfs:label "Credit Card Payment Method"@en ;      
        rdfs:comment """The credit card payment method refers to a financial transaction in which a consumer utilizes a credit card to purchase goods or services."""@en ;
        rdfs:subClassOf prop:PaymentMethod;
        rdfs:isDefinedBy  prop: .

prop:OperatorContractPaymentMethod a owl:Class ;
        rdfs:label "Operator Contract Payment Method"@en ;      
        rdfs:comment """The operator contract payment method  refers to the method by which payments are processed within the context of a contractual agreement between an operator (such as a charging point opertor) and another party (such as a client, customer, or partner)."""@en ;
        rdfs:subClassOf prop:PaymentMethod;
        rdfs:isDefinedBy  prop: .

