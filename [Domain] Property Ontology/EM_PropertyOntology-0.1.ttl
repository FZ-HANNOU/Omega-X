#-- Status

prop:Status a owl:Class ;
	rdfs:label "Status"@en ;
        rdfs:comment """TBA."""@en ; 
        rdfs:subClassOf plt:Status, prop:QualitativeProperty;
        rdfs:isDefinedBy prop: .

prop:AvailabilityStatus a owl:Class ;
	rdfs:label "Availablility Status"@en ;
        rdfs:comment """The availability status refers the current condition of a specific system at a given moment during a process, indicating whether it is operationally ready and accessible for use."""@en ; 
        rdfs:subClassOf prop:Status;
        rdfs:isDefinedBy prop: .

prop:BusyStatus a owl:Class ;
        rdfs:label "Busy Status"@en ;
        rdfs:comment """The busy status indicates that a system is currently free or occupied in time during a process."""@en ;  
        rdfs:subClassOf prop:Status;
        rdfs:isDefinedBy prop: .

prop:OccupiedStatus a owl:Class ;
        rdfs:label "Occupied Status"@en ;
        rdfs:comment """The occupied status is the situation of a system when it is actively in use."""@en ;  
        rdfs:subClassOf prop:BusyStatus;
        rdfs:isDefinedBy prop: .

prop:FreeStatus a owl:Class ;
        rdfs:label "Free Status"@en ;
        rdfs:comment """The free status refers to the situation of a system when it is available and not currently in use."""@en ;  
        rdfs:subClassOf prop:BusyStatus;
        rdfs:isDefinedBy prop:  .

prop:AuthenticationStatus a owl:Class ;
        rdfs:label "Authentication Status"@en ;
        rdfs:comment """The authentication status indicates whether an entity attempting access has been successfully verified and authorized or if the authentication attempt has failed, at a given moment during a process."""@en ; 
        rdfs:subClassOf prop:Status;
        rdfs:isDefinedBy prop:  .

prop:AuthorizedAuthenticationStatus a owl:Class ;
        rdfs:label "Authorized Authentication Status"@en ;
        rdfs:comment """The authorized authentication status refers to the confirmation of the authentication process, at a given moment during a process."""@en ; 
        rdfs:subClassOf prop:AuthenticationStatus;
        rdfs:isDefinedBy prop:  .


prop:UnauthorizedAuthenticationStatus a owl:Class ;
        rdfs:label "Unauthorized Authentication Status"@en ;
        rdfs:comment """The unauthorized authentication status refers to the disconfirmation of the authentication process, at a given moment during a process."""@en ; 
        rdfs:subClassOf prop:AuthenticationStatus;
        rdfs:isDefinedBy prop:  .

prop:ChargingStatus a owl:Class ;
        rdfs:label "Charging Status"@en ;
        rdfs:comment """The charging status refers the current condition of an electric vehicle's battery charging process, including whether it is actively charging, completed or reserved, at a given moment"""@en ; 
        rdfs:subClassOf prop:Status;
        rdfs:isDefinedBy prop: .

prop:CompletedChargingStatus a owl:Class ;
        rdfs:label "Completed Charging Status"@en ;
        rdfs:comment """The completed charging status refers the current condition of an electric vehicle's battery charging process when it is completed, at a given moment"""@en ; 
        rdfs:subClassOf prop:ChargingStatus;
        rdfs:isDefinedBy prop: .

prop:ReservedChargingStatus a owl:Class ;
        rdfs:label "Reserved Charging Status"@en ;
        rdfs:comment """The completed charging status refers the current condition of an electric vehicle's battery charging process when it is reserved, at a given moment"""@en ; 
        rdfs:subClassOf prop:ChargingStatus;
        rdfs:isDefinedBy prop: .

prop:BookingStatus a owl:Class ;
        rdfs:label "Booking Status"@en ;
        rdfs:comment """The booking status denotes the current status of a reservation, indicating whether it is confirmed, pending, canceled, or completed, at a given moment"""@en ; 
        rdfs:subClassOf prop:Status;
        rdfs:isDefinedBy prop: .

prop:PendingBookingStatus a owl:Class ;
        rdfs:label "Pending Booking Status"@en ;
        rdfs:comment """The pending booking status indicates that a reservation request has been submitted but has not yet been confirmed or finalized, at a given moment"""@en ; 
        rdfs:subClassOf prop:BookingStatus;
        rdfs:isDefinedBy prop: .

prop:ConfirmedBookingStatus a owl:Class ;
        rdfs:label "Confirmed Booking Status"@en ;
        rdfs:comment """The confirmed booking status indicates that a reservation has been successfully approved, at a given moment."""@en ; 
        rdfs:subClassOf prop:BookingStatus;
        rdfs:isDefinedBy prop: .

prop:RefusedBookingStatus a owl:Class ;
        rdfs:label "Refused Booking Status"@en ;
        rdfs:comment """The refused booking status indicates that a reservation request has been declined or rejected, at a given moment."""@en ; 
        rdfs:subClassOf prop:BookingStatus;
        rdfs:isDefinedBy prop: .

prop:CancelledBookingStatus a owl:Class ;
        rdfs:label "Cancelled Booking Status"@en ;
        rdfs:comment """The cancelled booking status indicates that a previously confirmed reservation has been terminated or invalidated, either by a user or service provider or an operator, at a given moment."""@en ; 
        rdfs:subClassOf prop:BookingStatus;
        rdfs:isDefinedBy prop: .

prop:ConfirmedCancelledBookingStatus a owl:Class ;
        rdfs:label "Confirmed Cancelled Booking Status"@en ;
        rdfs:comment """The confirmed cancelled booking status indicates a reservation that was previously confirmed but has been subsequently revoked or terminated before the scheduled date and time."""@en ; 
        rdfs:subClassOf prop:CancelledBookingStatus;
        rdfs:isDefinedBy prop: .

prop:RefusedCancelledBookingStatus a owl:Class ;
        rdfs:label "Refused Cancelled Booking Status"@en ;
        rdfs:comment """The refused cancelled booking status indicates a reservation request that was declined by the booking system or service provider after it had been confirmed and subsequently canceled by either the requester or the service provider, at a given moment."""@en ; 
        rdfs:subClassOf prop:CancelledBookingStatus;
        rdfs:isDefinedBy prop: .

prop:NoShowBookingStatus a owl:Class ;
        rdfs:label "No Show Booking Status"@en ;
        rdfs:comment """The no show booking status indicates a reservation where the requester did not attend the scheduled appointment or event without prior cancellation notification, at a given moment."""@en ; 
        rdfs:subClassOf prop:BookingStatus;
        rdfs:isDefinedBy prop:  .

prop:CompletedBookingStatus a owl:Class ;
        rdfs:label "Completed Booking Status"@en ;
        rdfs:comment """The completed booking status refers to the reservation has been successfully executed according to the scheduled time and specifications, at a given moment."""@en ; 
        rdfs:subClassOf prop:BookingStatus;
        rdfs:isDefinedBy prop: .

prop:AbortedBookingStatus a owl:Class ;
        rdfs:label "Aborted Booking Status"@en ;
        rdfs:comment """The aborted booking status signifies a reservation that was initiated but not completed due to unforeseen circumstances or interruptions, at a given moment."""@en ; 
