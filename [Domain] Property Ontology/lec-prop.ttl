#--LEC Battery static properties----------------------------------------------------------
:batteryProperty  a owl:Class ;
				        rdfs:subClassOf seas:Property , prop:QuantitaiveProperty;
						rdfs:comment "data sheet/static property related to a battery."@en.
						
:batteryNominalVoltage a owl:Class ;
						rdfs:subClassOf seas:Voltage , prop:batteryroperty;
						rdfs:comment "The operating voltage under load is 3V."@en ;
						rdfs:label "battery nominal voltage"@en ;
						prop:measuredIn unit:V;
						rdfs:isDefinedBy prop:.
:batteryOperatingVoltage a owl:Class ;
						rdfs:subClassOf seas:Voltage , prop:batteryProperty;
						rdfs:comment "The operating volatge at 80% DOD."@en ;
						rdfs:label "battery operating voltage"@en ;
						prop:measuredIn unit:V;
						rdfs:isDefinedBy prop:.
:batteryMinSoC a owl:Class ;
						rdfs:subClassOf prop:SoC, prop:QuantitaiveProperty;
						rdfs:comment "The minimum state of charge."@en ;
						rdfs:label "battery minimum state of charge"@en ;
						prop:measuredIn unit:PERCENT;
						rdfs:isDefinedBy prop:.
:batteryMaxSoC a owl:Class ;
						rdfs:subClassOf prop:SoC, prop:QuantitaiveProperty;
						rdfs:comment "The maximum state of charge."@en ;
						rdfs:label "battery maximum state of charge"@en ;
						prop:measuredIn unit:PERCENT;
						rdfs:isDefinedBy prop:.
:deepDischargeVoltage a owl:Class ;
						rdfs:subClassOf  seas:Volatge, prop:QuantitaiveProperty;
						rdfs:comment "The cell is damaged if voltage drops below this voltage."@en ;
						rdfs:label "deep discharge voltage"@en ;
						prop:measuredIn unit:V;
						rdfs:isDefinedBy prop:.
:batteryCapacity a owl:Class ;
						rdfs:subClassOf  prop:QuantitaiveProperty;
						rdfs:comment "The capacity of the battery in ampere hour."@en ;
						rdfs:label "battery capacity"@en ;
						prop:measuredIn unit:A-HR;
						rdfs:isDefinedBy prop:.
:batteryCellAverageTemperature a owl:Class ;
						rdfs:subClassOf  seas:Temperature, prop:QuantitaiveProperty;
						rdfs:comment "The battery temperature should not decrease this level during charge and discharge."@en ;
						rdfs:label "battery cell average temperature"@en ;
						prop:measuredIn unit:DEG_C;
						rdfs:isDefinedBy prop:.
:batteryMaxTemperature a owl:Class ;
						rdfs:subClassOf  seas:Temperature, prop:QuantitaiveProperty;
						rdfs:comment "The battery temperature should not increase this level during charge and discharge."@en ;
						rdfs:label "battery cell average temperature"@en ;
						prop:measuredIn unit:DEG_C;
						rdfs:isDefinedBy prop:.
:batteryMaxChargeVoltage a owl:Class ;
						rdfs:subClassOf  seas:Voltage, prop:QuantitaiveProperty;
						rdfs:comment "The battery cell si damaged if voltage exceeds thsi level."@en ;
						rdfs:label "battery max charge voltage"@en ;
						prop:measuredIn unit:V;
						rdfs:isDefinedBy prop:.
:batteryOptimalDischargeCurrent a owl:Class ;
						rdfs:subClassOf  seas:Current, prop:QuantitaiveProperty;
						rdfs:comment "The maximal  current at 0.5 C."@en ;
						rdfs:label "battery optimal discharge current"@en ;
						prop:measuredIn unit:A;
						rdfs:isDefinedBy prop:.
:batteryMaxDischargeCurrent a owl:Class ;
						rdfs:subClassOf  seas:Current, prop:QuantitaiveProperty;
						rdfs:comment "The maximal current at 3 C, continious for max 15 minutes from full charge."@en ;
						rdfs:label "battery optimal discharge current"@en ;
						prop:measuredIn unit:A;
						rdfs:isDefinedBy prop:.
:batteryMaxPeakDischargeCurrent a owl:Class ;
						rdfs:subClassOf  seas:Current, prop:QuantitaiveProperty;
						rdfs:comment "The maximal current at 10 C, maximal 5 seconds in 1 minute."@en ;
						rdfs:label "battery max peak discharge current"@en ;
						prop:measuredIn unit:A;
						rdfs:isDefinedBy prop:.
:batteryOptimalChargeCurrent a owl:Class ;
						rdfs:subClassOf  seas:Current, prop:QuantitaiveProperty;
						rdfs:comment "The maximal current at 0.5 C."@en ;
						rdfs:label "battery optimal charge current"@en ;
						prop:measuredIn unit:A;
						rdfs:isDefinedBy prop:.
:batteryMaChargeCurrent a owl:Class ;
						rdfs:subClassOf  seas:Current, prop:QuantitaiveProperty;
						rdfs:comment "the maximal current at <0.3 C with battery temperature monitoring."@en ;
						rdfs:label "battery mac charge current"@en ;
						prop:measuredIn unit:A;
						rdfs:isDefinedBy prop:.
:batteryDimensions a owl:Class ;
						rdfs:subClassOf prop:QuantitaiveProperty;
						rdfs:comment "The dimension of the battery Lxlxh."@en ;
						rdfs:label "battery dimensions"@en ;
						prop:measuredIn unit:MilliM;
						rdfs:isDefinedBy prop:.
:batteryWeight  a owl:Class ;
						rdfs:subClassOf prop:QuantitaiveProperty;
						rdfs:comment "The weight of the battery in Kg."@en ;
						rdfs:label "battery weight"@en ;
						prop:measuredIn unit:KiloGM;
						rdfs:isDefinedBy prop:.

