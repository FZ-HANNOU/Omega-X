#-- Payment Method

prop:PaymentMethod a owl:Class ;
        rdfs:label "Payment Method"@en ;      
        rdfs:comment "The payment method refers to the various ways can pay for services, infrastructure use, or offerings."@en ;
        rdfs:subClassOf prop:QualitativeProperty;
	prop:isEvaluatedIn prop:PaymentModeCategory;
        rdfs:isDefinedBy  prop: .
       
prop:PaymentMethodCategory a owl:Class ;
        rdfs:label "Payment Method Category"@en ;
        rdfs:comment "List of identified values for the payment method."@en ;  
        rdfs:subClassOf prop:Category;
	rdfs:isDefinedBy prop: .
  
prop:FreeOfPayment a owl:NamedIndividual, prop:PaymentMethodCategory ;
        rdfs:label "Free"@en ;      
        rdfs:comment "The free payment method reefrs to a transactional process where the payer does not incur any direct financial cost or obligation when making a payment."@en ;
        rdfs:isDefinedBy  prop: .

prop:CreditCard a owl:NamedIndividual, prop:PaymentMethodCategory ;
        rdfs:label "Credit Card"@en ;      
        rdfs:comment "The credit card payment method refers to a financial transaction in which a consumer utilizes a credit card to purchase goods or services."@en ;
        rdfs:isDefinedBy  prop: .

prop:OperatorContract a owl:NamedIndividual, prop:PaymentMethodCategory;
        rdfs:label "Operator Contract"@en ;      
        rdfs:comment "The operator contract payment method  refers to the method by which payments are processed within the context of a contractual agreement between an operator (such as a charging point opertor) and another party (such as a client, customer, or partner)."@en ;
        rdfs:isDefinedBy  prop: .

#-- Status

prop:Status a owl:Class ;
	rdfs:label "Status"@en ;
        rdfs:comment "TBA."@en ; 
        rdfs:subClassOf plt:Status, prop:QualitativeProperty;
        rdfs:isDefinedBy prop: .

#-- Availability Status

prop:AvailabilityStatus a owl:Class ;
	rdfs:label "Availablility Status"@en ;
        rdfs:comment "The availability status refers the current condition of a specific system at a given moment during a process, indicating whether it is operationally ready and accessible for use."@en ; 
        rdfs:subClassOf prop:Status;
        prop:isEvaluatedIn prop:AvailabilityStatusCategory;
	rdfs:isDefinedBy prop: .

prop:AvailabilityStatusCategory a owl:Class ;
        rdfs:label "Availability Status Category"@en ;
        rdfs:comment "List of identified values for the availability status."@en ;  
        rdfs:subClassOf prop:Category;
	rdfs:isDefinedBy prop: .

prop:OutOfOrder a owl:NamedIndividual, prop:AvailabilityStatusCategory ;
	rdfs:label "Out Of Order"@en ;
        rdfs:comment "(source eMIP) The EVCI element is not available to the end-user."@en ; 
        rdfs:seeAlso <https://www.gireve.com/wp-content/uploads/2022/09/Gireve_Tech_eMIP-V0.7.4_ImplementationGuide_1.0.7_en.pdf>;
        rdfs:isDefinedBy prop: .

prop:InService a owl:NamedIndividual, prop:AvailabilityStatusCategory ;
	rdfs:label "In Service"@en ;
        rdfs:comment "(source eMIP) The EVCI element is available to the end-user."@en ; 
        rdfs:seeAlso <https://www.gireve.com/wp-content/uploads/2022/09/Gireve_Tech_eMIP-V0.7.4_ImplementationGuide_1.0.7_en.pdf>;
        rdfs:isDefinedBy prop: .

prop:Future a owl:NamedIndividual, prop:AvailabilityStatusCategory ;
	rdfs:label "Future"@en ;
        rdfs:comment "(source eMIP) The EVCI element is not available to the end-user because it does not exist yet. The installation of this element is scheduled."@en ; 
        rdfs:seeAlso <https://www.gireve.com/wp-content/uploads/2022/09/Gireve_Tech_eMIP-V0.7.4_ImplementationGuide_1.0.7_en.pdf>;
        rdfs:isDefinedBy prop: .

prop:Deleted a owl:NamedIndividual, prop:AvailabilityStatusCategory ;
	rdfs:label "Deleted"@en ;
        rdfs:comment "(source eMIP) The EVCI element is not available to the end-user because it does not exist anymore."@en ; 
        rdfs:seeAlso <https://www.gireve.com/wp-content/uploads/2022/09/Gireve_Tech_eMIP-V0.7.4_ImplementationGuide_1.0.7_en.pdf>;
        rdfs:isDefinedBy prop: .

#-- Occupancy Status

prop:OccupancyStatus a owl:Class ;
        rdfs:label "Occupancy Status"@en ;
        rdfs:comment "The occupancy status indicates that a system is currently free or occupied in time during a process."@en ;  
        rdfs:subClassOf prop:Status;
        prop:isEvaluatedIn prop:OccupancyStatusCategory;
	rdfs:isDefinedBy prop: .

prop:OccupancyStatusCategory a owl:Class ;
        rdfs:label "Occupancy Status Category"@en ;
        rdfs:comment "List of identified values for the busy status."@en ;  
        rdfs:subClassOf prop:Category;
	rdfs:isDefinedBy prop: .

prop:Occupied a owl:NamedIndividual, prop:BusyStatusCategory;
        rdfs:label "Occupied"@en ;
        rdfs:comment "The occupied status is the situation of a system when it is actively in use."@en ;  
        rdfs:isDefinedBy prop: .

prop:FreeOfOccupancy a owl:NamedIndividual, prop:BusyStatusCategory ;
        rdfs:label "Free"@en ;
        rdfs:comment "The free status refers to the situation of a system when it is available and not currently in use."@en ;  
        rdfs:isDefinedBy prop:  .


