prop:ReactivePowerProduced a owl:Class ;
		                   rdfs:comment "The reactive power produced."@en ;
		                   rdfs:label "Reactive Power Produced"@en ;
		                   rdfs:subClassOf  prop:ReactivePower;
                           rdfs:isDefinedBy prop:.
# Reactive Power Consumed
prop:ReactivePowerConsumed a owl:Class ;
		                   rdfs:comment "The reactive power consumed."@en ;
		                   rdfs:label "Reactive Power Consumed"@en ;
		                   rdfs:subClassOf  prop:ReactivePower;
