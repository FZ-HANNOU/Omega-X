#---- REN Solar Module properties------------------------------------------------------------
prop:solarModuleProperty a owl:Class ;
		                rdfs:subClassOf prop:Property;
		                rdfs:comment "This property includes all static properties of a solar module."@en ;
	  	                rdfs:label "Solar Module Property"@en ;
		                rdfs:isDefinedBy prop:.
prop:solarModuleVoC rdf:type owl:Class ;
							rdfs:comment "Solar module open circuit voltage given at STC operating standard conditions."@en ;
							rdfs:label "Solar Module VOC"@en ;
							rdfs:subClassOf prop:Voltage,  prop:solarModuleProperty;
							prop:measuredIn unit:V;
							rdfs:isDefinedBy prop:.
prop:solarModuleIsc rdf:type owl:Class ;
							rdfs:comment "Solar module short circuit current given at STC operating standard conditions."@en ;
							rdfs:label "Solar Module ISC"@en ;
							rdfs:subClassOf prop:Current, prop:solarModule;
							prop:measuredIn unit:A;
							rdfs:isDefinedBy prop:.
prop:solarModuleVmp rdf:type owl:Class ;
							rdfs:comment "Solar module maximum power point voltage at operating standard conditions."@en ;
							rdfs:label "Solar Module VMP"@en ;
							rdfs:subClassOf prop:Voltage, prop:solarModuleProperty;
							prop:measuredIn unit:V;
							rdfs:isDefinedBy prop:.
prop:solarModuleImp rdf:type owl:Class ;
							rdfs:comment "Solar module maximum power point current at operating standard conditions."@en ;
							rdfs:label "Solar Module ISC"@en ;
							rdfs:subClassOf prop:Current, prop:solarModuleProperty;
							prop:measuredIn unit:A;
							rdfs:isDefinedBy prop:.

prop:solarModuleTemperatureCoefficient rdf:type owl:Class ;
							rdfs:comment "Solar module temperature coefficient used to consider performance degradation due to high temperature."@en ;
							rdfs:label "Solar Module Temperature Coefficient"@en ;
							rdfs:subClassOf prop:Temperature, prop:solarModuleProperty;
							prop:measuredIn unit:DEG_C;
							rdfs:isDefinedBy prop:.
