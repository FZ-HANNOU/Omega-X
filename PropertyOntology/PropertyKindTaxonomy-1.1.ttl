			 skos:broader prop:BatteryOperatingVoltage;
			 skos:definition "Battery minimum operating voltage at 80% Depth Of Discharge (DOD)."@en ;
			  skos:prefLabel "Battery Minimum Operating Voltage"@en .
			  
# Battery Minimum State of Charge               
prop:BatteryMinSoC a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
	           skos:broader  prop:StateOfCharge;
	           skos:definition "Battery minimum state of charge."@en ;
	            skos:prefLabel "Battery Minimum State of Charge"@en .
		    
# Battery Maximum State of Charge
prop:BatteryMaxSoC a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
