#--------------------------------------------------------------------------------------------------------------
#                       Taxonomy of Energy Domain Properties
#--------------------------------------------------------------------------------------------------------------

# Quantitative Properties
#----------------------------------------------------------------
# Temperature
prop:Temperature a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty ; 
              skos:prefLabel "Temperature"@en;
			 prop:isMeasuredIn unit:DEG_C . 
# Energy
prop:Energy a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
              skos:prefLabel "Energy"@en.
              
# Power
prop:Power a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
              skos:prefLabel "Power"@en.
              
# Current
prop:Current a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
              skos:prefLabel "Current"@en;
			 prop:isMeasuredIn unit:A.

#Capacity 
prop:Capacity a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
              skos:prefLabel "Capacity"@en.
              
# DC current
prop:DCCurrent a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
             skos:broader prop:Current ; 
             skos:definition "The current in DC mode."@en ; 
            skos:prefLabel "DC Current"@en;
			 prop:isMeasuredIn unit:A.
              
# Resistance 
prop:Resistance a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
            skos:definition "For a resistive two-terminal element or two-terminal circuit with terminals A and B, quotient of the voltage (131-11-56) uAB between the terminals by the electric current i in the element or circuit."@en ; 
            skos:prefLabel "Resistance"@en;
			dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=131-12-04>;
			prop:isMeasuredIn unit:OHM.
             

# AC Current
prop:ACCurrent a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
             skos:broader prop:Current  ; 
             skos:definition "Alternating current on different phases."@en ; 
              skos:prefLabel "AC Current L1"@en.
              
# Voltage
prop:Voltage a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
             skos:prefLabel "Voltage"@en;
			 prop:isMeasuredIn unit:V.
              
# Irradiance  
prop:Irradiance a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;  
             skos:definition "density of incident radiant flux with respect to area at a point on a real or imaginary surface."@en ; 
			 dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=845-21-053>;
             skos:prefLabel "Irradiance"@en;
			 prop:isMeasuredIn unit:W-PER-M2;
			 prop:isDefinedBy prop:.
# DCVoltage
prop:DCVoltage a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
             skos:broader prop:Voltage ; 
             skos:definition "Direct Current Voltage"@en ; 
              skos:prefLabel "DC Voltage"@en.
              
# AC Voltage phase 
prop:ACVoltage a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
             skos:broader prop:Voltage ; 
             skos:definition "Alternating Current Voltage for different phases."@en ; 
             skos:prefLabel "AC Voltage"@en.
              			
# Root Mean Square Voltage
prop:RMSVoltage a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		     skos:definition "The root mean square voltage."@en ;
		     skos:prefLabel "RMS Voltage"@en ;
		     skos:broader prop:Voltage.
		      
# Frequency
prop:Frequency a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
             skos:definition "Reciprocal of the period."@en ; 
              skos:prefLabel "Frequency"@en;
			 prop:isMeasuredIn unit:HZ.
              
# Energy Cost
prop:EnergyCost a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		skos:definition "Energy prices agreed with the supplier for the energy term."@en ;
		 skos:prefLabel "Energy Cost"@en ;
		prop:isMeasuredIn unit:EUR-PER-KiloW.
		 

# Air Temperature
prop:AirTemperature a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
        skos:broader prop:Temperature ;   
		prop:isMeasuredIn unit:DEG_C;
        skos:prefLabel "Air Temperature"@en.

# Heat Sink Temperature
prop:HeatSinkTemperature a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
                         skos:broader prop:Temperature ;    
				         prop:isMeasuredIn unit:DEG_C;
                          skos:note "used for dinv1.heatsinktmp IEC 61850 tag"@en;
                          skos:prefLabel "Heat Sink Temperature"@en.
# Enclosure Temperature
prop:EnclosureTemperature a owl:namedIndividual , skos:Concept , prop:QuantitativeProperty  ;
                         skos:broader prop:Temperature ;    
				         prop:isMeasuredIn unit:DEG_C;
                          skos:prefLabel "Enclosure Temperature"@en.

# System Internal Temperature
prop:SystemInternalTemperature a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			skos:broader prop:Temperature ; 
            skos:prefLabel "System Internal Temperature"@en.
			 

# Back of Panel Temperature
prop:BackOfPanelTemperature a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			skos:broader prop:Temperature ; 
            skos:definition "The temperature of a panel module measured by a weather station."@en ; 
            skos:prefLabel "Back of Panel Temperature"@en.
			 

# Internal Inverter Temperature
prop:InternalInverterTemperature a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			skos:broader prop:Temperature ; 
            skos:definition "Maximum temperatures among all the Power Electronics devices constituting the inverter arms (IEC_61850_7_420_2019A)."@en ; 
            skos:prefLabel "Internal Inverter Temperature"@en.
			 

# Electrical losses
prop:Elosses a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
	     skos:definition "The estimated amount of energy lost in the distribution grid."@en ;
	      skos:prefLabel "Electrical Losses"@en ;
	     skos:broader prop:Energy;
	     prop:isMeasuredIn unit:KiloW-HR.

# Thermal Energy
prop:ThermalEnergy  a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		skos:definition "Thermal energy is a kind of energy and it is generated when the temperature rises. It can be consumed or supplied."@en ;
		skos:broader seas:ElectricalEnergyProperty, prop:Energy;
		 skos:prefLabel "Thermal Energy "@en ;
		prop:isMeasuredIn unit:KiloW-HR.
		 
                
# Water Quantity                
prop:WaterQuantity a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
	    skos:definition "Volume flow rate quantity equal to the volume dV of substance crossing a given surface during a time interval with infinitesimal duration dt, divided by this duration, thus qV = dV / dt."@en ;
		 skos:prefLabel "Water Losses "@en ;
		dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=113-03-72>;
		prop:isMeasuredIn unit:M3-PER-HR.
		 
# Water Losses
prop:WayerLosses a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
	         skos:definition "The amount of water lost in the distribution grid. It can be consumed or supplied."@en ;
	         skos:broader prop:WaterQuantity;
		  skos:prefLabel "Water Losses "@en .
		  

# Water Supplied
prop:WaterSupplied a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
                   skos:definition "The water quantity supplied."@en ;
	            skos:prefLabel "Water Supplied"@en ;
	           skos:broader prop:WaterQuantity.
	            

# Water Consumed               
prop:WaterConsumed a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
                   skos:definition "The water quantity consumed."@en ;
		    skos:prefLabel "Water Consumed"@en ;
		   skos:broader prop:WaterQuantity.
		    

# Water Losses
prop:WaterLosses a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
             skos:definition "The computed water quantity lost."@en ;
	      skos:prefLabel "Water Losses"@en ;
	     skos:broader prop:WaterQuantity.
	      

# Active Energy
prop:ActiveEnergy a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		          skos:definition "the active energy is the integral of the instantaneous power p over a time interval [t1, t2]."@en ;
		           skos:prefLabel "Active Energy"@en ;
		          skos:broader seas:ElectricEnergyProperty, prop:Energy;
		          dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=131-11-57>;
		          prop:isMeasuredIn unit:KiloW-HR, unit:W-HR.
                   
# Active Energy Produced
prop:ActiveEnergyProduced a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		                  skos:definition "the active energy produced."@en ;
		                   skos:prefLabel "Active Energy Produced"@en ;
		                  skos:broader prop:ActiveEnergy.
						   
# Active Energy Consumed
prop:ActiveEnergyConsumed a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		                  skos:definition "the active energy consumed."@en ;
		                   skos:prefLabel "Active Energy Consumed"@en ;
		                  skos:broader prop:ActiveEnergy.
						   

# Active Energy Produced Off Peak Hour 
prop:ActiveEnergyProducedOffPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		                  skos:definition "the active energy produced during off peak hour."@en ;
		                   skos:prefLabel "Active Energy Produced Off Peak Hour"@en ;
		                  skos:broader prop:ActiveEnergyProduced.

# Active Energy Produced  Peak Hour 
prop:ActiveEnergyProducedPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		                  skos:definition "the active energy produced during  peak hour."@en ;
		                   skos:prefLabel "Active Energy Produced Peak Hour"@en ;
		                  skos:broader prop:ActiveEnergyProduced.

# Reactive Energy							
prop:ReactiveEnergy a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		    skos:definition "In an AC system, the captive electrical energy exchanged continuously between the different electric and magnetic fields associated with the operation of the electrical system and of all the connected apparatus."@en ;
		     skos:prefLabel "Reactive Energy"@en ;
		    skos:broader seas:ElectricEnergyProperty, prop:Energy;
		    dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=601-01-20>.
		     
# Reactive Energy Consumed						
prop:ReactiveEnergyConsumed a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		    skos:definition "The reactive energy consumed."@en ;
		     skos:prefLabel "Reactive Energy Consumed"@en ;
		    skos:broader prop:ReactiveEnergy;
			prop:isMeasuredIn unit:KiloW-HR.
		     
# Reactive Energy Consumed OFF Peak Hour						
prop:ReactiveEnergyConsumedOffPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		    skos:definition "The reactive energy consumed during off-peak hours."@en ;
		     skos:prefLabel "Reactive Energy Consumed Off Peak Hours"@en ;
		    skos:broader prop:ReactiveEnergyConsumed.
		     
# Reactive Energy Consumed  Peak Hour						
prop:ReactiveEnergyConsumedPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		    skos:definition "The reactive energy consumed during peak hours."@en ;
		     skos:prefLabel "Reactive Energy Consumed Peak Hours"@en ;
		    skos:broader prop:ReactiveEnergyConsumed.
		     
#Reactive Energy Produced						
prop:ReactiveEnergyProduced a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		    skos:definition "The reactive energy produced."@en ;
		     skos:prefLabel "Reactive Energy Produced"@en ;
		    skos:broader prop:ReactiveEnergy;
			prop:isMeasuredIn unit:V-A_Reactive-HR.
		     
# Reactive Energy Produced OFF Peak Hour						
prop:ReactiveEnergyProducedOffPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		    skos:definition "The reactive energy produced during off-peak hours."@en ;
		     skos:prefLabel "Reactive Energy Produced Off Peak Hours"@en ;
		    skos:broader prop:ReactiveEnergyProduced.
		     
# Reactive Energy Produced  Peak Hour						
prop:ReactiveEnergyProducedPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		    skos:definition "The reactive energy produced during peak hours."@en ;
		     skos:prefLabel "Reactive Energy Produced Peak Hours"@en ;
		    skos:broader prop:ReactiveEnergyProduced.
		     
# Active Power
prop:ActivePower a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		 skos:definition "Under periodic conditions, mean value, taken over one period T, of the instantaneous power p."@en ;
		  skos:prefLabel "Active Power"@en ;
		 skos:broader seas:ElectricPoweryProperty, prop:Power;
		 dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=131-11-42>;
		 prop:isMeasuredIn unit:W.
		  
# Active Power AC
prop:ActivePowerAC a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		 skos:definition "Active Power in AC Mode."@en ;
		  skos:prefLabel "Active Power AC"@en ;
		 skos:broader  prop:ActivePower.
		  
# Active Power DC
prop:ActivePowerDC a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		 skos:definition "Active Power in DC Mode."@en ;
		  skos:prefLabel "Active Power DC"@en ;
		 skos:broader  prop:ActivePower.
		  
# Active Power Produced
prop:ActivePowerProduced a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		 skos:definition "Active Power Produced."@en ;
		  skos:prefLabel "Active Power Produced"@en ;
		 skos:broader  prop:ActivePower.
		  

# Reactive Power
prop:ReactivePower a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		   skos:definition "Quantity equal to the mean value of the product of the instantaneous voltage u and the instantaneous current i which is equal to i but leading it by π/2."@en ;
		   skos:prefLabel "Reactive Power"@en ;
		   skos:broader seas:ElectricPoweryProperty, prop:Power;
		   dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=131-11-44>;
		   prop:isMeasuredIn unit:V-A_Reactive.
                    
# Reactive Power Produced
prop:ReactivePowerProduced a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		                   skos:definition "The reactive power produced."@en ;
		                    skos:prefLabel "Reactive Power Produced"@en ;
		                   skos:broader  prop:ReactivePower.
                            
# Reactive Power Consumed
prop:ReactivePowerConsumed a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		                   skos:definition "The reactive power consumed."@en ;
		                    skos:prefLabel "Reactive Power Consumed"@en ;
		                   skos:broader  prop:ReactivePower.
                            
# Reactive Power Set Point
prop:ReactivePowerSetPoint a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			   skos:definition "Predetermined or desired value of reactive power that a device or electrical system should produce or consume at a given time."@en ;
			    skos:prefLabel "Reactive Power Set Point"@en ;
			   skos:broader seas:ElectricPowerProperty, prop:Power;
			   prop:isMeasuredIn unit:V-A_Reactive.
			    
# DC Power
prop:DCPower a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
	     skos:definition "Power in DC mode."@en ;
	      skos:prefLabel "DC Power"@en ;
	     skos:broader seas:ElectricPowerProperty, prop:Power;
	     prop:isMeasuredIn unit:W.
              
# Energy Price
prop:EnergyPrice a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		 skos:definition "Energy prices agreed with the DSO for the power term ."@en ;
		  skos:prefLabel "Energy Price"@en ;
		 skos:broader seas:PricePerEnergy;
		 prop:isMeasuredIn unit:EUR-PER-KiloW.
                  
# State of Power 
prop:StateOfPower a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
	 	skos:definition "A battery state-of-power (SOP) is defined as the ratio of peak power to nominal power ."@en ;
        dcterms:source <https://epicpower.es/wp-content/uploads/2020/08/AN028_SoC-SoH-SoP-definitions_v3.pdf>;
	  	skos:prefLabel "State of Power"@en ;
	 	skos:broader prop:Power;
	 	prop:isMeasuredIn unit:W.
          
# State of Power in Charge
prop:StateofPowerCharge a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
            skos:definition "The maximum possible charge power that a battery system can support over a fixed time interval."@en ;
			skos:prefLabel "State of Power in Charge"@en ;
			skos:broader prop:StateOfPower.
			 
# State of Power in Discharge
prop:StateofPowerDischarge a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			skos:definition "The maximum possible discharge power that a battery system can support over a fixed time interval."@en ;
			skos:prefLabel "State of Power in Discharge"@en ;
			skos:broader prop:StateOfPower.
			    
# State of Charge
prop:StateOfCharge  a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
		   skos:definition "The state of charge (SoC) can be described as the level of charge of a battery relative to its capacity.."@en ;
		    skos:prefLabel "State of Charge"@en ;
            dcterms:source <https://epicpower.es/wp-content/uploads/2020/08/AN028_SoC-SoH-SoP-definitions_v3.pdf>;
		   prop:isMeasuredIn unit:PERCENT.
		    
# Insulation Resistance
prop:InsulationResistance a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			   skos:definition "Resistance under specified conditions between two conductive elements separated by insulating materials ."@en ;
			    skos:prefLabel "Insulation Resistance"@en ;
			   dcterms:source <https://www.electropedia.org/iev/iev.nsf/display?openform&ievref=151-15-43>;
			   skos:broader prop:Resistance.
			    
#Production Duration Off Peak Hours
prop:ProductionDurationOffPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			   skos:definition "The duration of the electricity production during off peak hours."@en ;
			    skos:prefLabel "Production Duration Off Peak Hour"@en ;
			   prop:isMeasuredInIn time:Duration.
			    
#Production Duration  Peak Hours
prop:ProductionDurationPeakHR a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			   skos:definition "The duration of the electricity production during peak hours."@en ;
			    skos:prefLabel "Production Duration Peak Hour"@en ;
			   prop:isMeasuredInIn time:Duration.
			    
#Production Duration
prop:ProductionDuration a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty  ;
			   skos:definition "The duration of the electricity production during peak and peak-off hours."@en ;
			    skos:prefLabel "Production Duration"@en ;
			   prop:isMeasuredInIn time:Duration.

# Battery Property
prop:BatteryProperty  a owl:NamedIndividual , skos:Concept , prop:QuantitativeProperty ;
                      skos:definition "Data sheet/static property related to a battery."@en.
                       
						
